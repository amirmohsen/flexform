{
  "name": "flexform",
  "private": true,
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "npm install && npm prune",
    "watch": "webpack-dev-server --content-base dist/ --config tasks/build.js --inline --hot --port 3000 --progress --color --display-error-details --debug",
    "serve": "http-server ./examples",
    "dev": "npm run prepare && concurrently --kill-others \"npm run watch\" \"npm run serve\"",
    "build": "npm run prepare && webpack-dev-server --content-base dist/ --config tasks/build.js"
  },
  "dependencies": {
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-module-resolver": "^2.2.0",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-optimize": "^1.0.1",
    "clean-webpack-plugin": "^0.1.10",
    "concurrently": "^2.2.0",
    "css-loader": "^0.24.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "http-server": "^0.9.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.11.1",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^4.0.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1",
    "yargs": "^5.0.0"
  },
  "babel": {
    "presets": [
      "latest",
      "react",
      "react-optimize"
    ],
    "plugins": [
      "syntax-decorators",
      "transform-decorators-legacy",
      "transform-class-properties",
      "transform-object-rest-spread",
      "transform-object-assign",
      "array-includes",
      "add-module-exports",
      [
        "module-resolver",
        {
          "root": [
            "./"
          ]
        }
      ]
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
