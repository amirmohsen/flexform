{
  "name": "flexform",
  "version": "0.0.3",
  "repository": "https://github.com/amirmohsen/flexform",
  "description": "A form generator library using FlexSchema",
  "scripts": {
    "build": "rollup -c -m",
    "dev": "rollup -c -m -w",
    "test": "jest",
    "pretest": "npm run build",
    "prepublishOnly": "npm run build",
    "examples": "http-server ./examples -c-1"
  },
  "files": [
    "dist"
  ],
  "main": "dist/flexschema.cjs.js",
  "module": "dist/flexschema.esm.js",
  "browser": "dist/flexschema.umd.js",
  "keywords": [
    "flexverse",
    "form",
    "generator"
  ],
  "author": {
    "name": "Amir Abdolrazaghi",
    "email": "amir@flexverse.tech"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amirmohsen/flexform/issues"
  },
  "homepage": "https://github.com/amirmohsen/flexform#readme",
  "babel": {
    "presets": [
      [
        "env"
      ],
      "react"
    ],
    "plugins": [
      "syntax-decorators",
      "transform-decorators-legacy",
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  },
  "dependencies": {
    "extend": "^3.0.0",
    "extendable-error": "^0.1.4",
    "flexschema": "^0.1",
    "lodash": "^4.17.4",
    "object-path-immutable": "^0.5.1"
  },
  "peerDependencies": {
    "prop-types": "^15.6.0",
    "react": "^16",
    "react-dom": "^16"
  },
  "devDependencies": {
    "babel-jest": "^21.2.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.3.3",
    "babel-preset-react": "^6.24.1",
    "http-server": "^0.10.0",
    "jest": "^21.2.1",
    "prop-types": "^15.6.0",
    "react": "^16",
    "react-dom": "^16",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1"
  }
}
